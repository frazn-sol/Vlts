  <%= stylesheet_link_tag "style.css" %>
  <%= javascript_include_tag "application" %><!-- 
  <%= javascript_include_tag "highcharts.js" %>   -->
  <%= javascript_include_tag "validate.min.js" %>
  <%=javascript_include_tag "autocomplete-rails.js"%>
  <%= csrf_meta_tags %>
