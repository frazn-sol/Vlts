.row-fluid.sortable
  .box.span12
    .box-header.well{"data-original-title" => ""}
      %h2
        %i.icon-user.icon-white
        -if params[:param]=="1"    
          View Supervisor
        -elsif params[:param]=="2"   
          View User
        -elsif params[:param]=="3"   
          View Profile  
        -else       
          View Details
    .box-content
      = render ("layouts/breadcrumbs")
      %p
        %b First name:
        = @user.first_name
      %p
        %b Middle name:
        = @user.middle_name
      %p
        %b Last name:
        = @user.last_name
      %p
        %b Phone:
        = @user.phone
      %p
        %b Cell:
        = @user.cell
      %p
        %b Email:
        = @user.email  
