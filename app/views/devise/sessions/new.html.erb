
<div class = "header"><h2>Login</h2> </div>

<%= form_for(resource, :as => resource_name, :url => session_path(resource_name)) do |f| %>
  <div class = "field">
  <%= f.label :email %><br />
  <%= f.email_field :email, :class => "up" %></div>

  	<div class = "field">
  <%= f.label :password %><br />
  <%= f.password_field :password, :class => "up" %></div>

  <%= f.submit "Login", :class => "login" %>
<% end %>
<div class = "partial">
<%= render :partial => "devise/shared/links" %>
</div>